<Window x:Class="virtual_aquarium.Screensaver"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:virtual_aquarium"
        mc:Ignorable="d"
        Title="Виртуальный аквариум" Height="1080" Width="1920" WindowState="Maximized" WindowStyle="None">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="33*"/>
            <RowDefinition Height="7*"/>
        </Grid.RowDefinitions>
        <Image Opacity="0" Name="img" Source="VAlogo.png"/>
        <Label Name="lbl" Grid.Row="1" Content="Нажмите Enter, чтобы продолжить" Style="{StaticResource Lb}"></Label>
    </Grid>
</Window>
